<config>
    <tabs>
        <yireo translate="label" module="adminpreviousnext">
            <label>Yireo</label>
            <sort_order>900</sort_order>
        </yireo>
    </tabs>
    <sections>
        <adminpreviousnext translate="label" module="adminpreviousnext">
            <label>Admin Previous Next</label>
